#lan_tran

<Query>
        
    BoxLayout:
        orientation: 'vertical'
     

        
        Label:
            background_color: 0, 0, 1, 1
            text: 'Lan_Tran'
            font_size: 40

        Label:
            text: "Word Lookup"
            font_size: 20
           
            
        TextInput:
            text: ''
            id: entry
            font_size: 20
            size_hint: [10,.9 ]

        
        Label:
            id: result
            font_size: 20
            

        Button:
            text: 'Translate'
            size_hint : [1, .5]
            on_press: root.engToLan()
    
       
        
            

